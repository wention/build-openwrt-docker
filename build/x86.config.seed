CONFIG_TARGET_x86=y
CONFIG_TARGET_x86_64=y
CONFIG_TARGET_x86_64_DEVICE_generic=y
# CONFIG_GRUB_IMAGES is not set
# CONFIG_VMDK_IMAGES is not set
CONFIG_TARGET_ROOTFS_PARTSIZE=750

CONFIG_ISO_IMAGES=y
CONFIG_QCOW2_IMAGES=y
CONFIG_VDI_IMAGES=y
CONFIG_VMDK_IMAGES=y
CONFIG_VHDX_IMAGES=y

# 访问控制
CONFIG_PACKAGE_luci-app-accesscontrol=y

# ACME SSL 证书
CONFIG_PACKAGE_luci-app-acme=y
CONFIG_PACKAGE_acme-deploy=y
CONFIG_PACKAGE_acme-dnsapi=y
CONFIG_PACKAGE_acme-notify=y

# 广告屏蔽大师 Plus
CONFIG_PACKAGE_luci-app-adbyby-plus=y

# arp 绑定
CONFIG_PACKAGE_luci-app-arpbind=y

# 自动重启
CONFIG_PACKAGE_luci-app-autoreboot=y

# DDNS 支持
CONFIG_PACKAGE_luci-app-ddns=y

# 核心诊断工具
CONFIG_PACKAGE_luci-app-diag-core=y

# dnscrypt-proxy
CONFIG_PACKAGE_luci-app-dnscrypt-proxy=y

# OpenClash
CONFIG_PACKAGE_luci-app-openclash=y

# ssr
CONFIG_PACKAGE_luci-app-ssr-plus=y

# OpenVPN 服务
CONFIG_PACKAGE_luci-app-openvpn-server=y

# 释放内存
CONFIG_PACKAGE_luci-app-ramfree=y

# 云盘同步
CONFIG_PACKAGE_luci-app-rclone=y

# 统计
CONFIG_PACKAGE_luci-app-statistics=y

# 组播转 UDP (IPTV)
CONFIG_PACKAGE_igmpproxy=y
CONFIG_PACKAGE_luci-app-udpxy=y

# KMS 服务
CONFIG_PACKAGE_luci-app-vlmcsd=y

#
CONFIG_PACKAGE_luci-app-sqm=y

# WakeOnLAN
CONFIG_PACKAGE_luci-app-wol=y
# Zerotier
CONFIG_PACKAGE_luci-app-zerotier=y


# VPN 连接支持库
CONFIG_PACKAGE_xl2tpd=y
CONFIG_PACKAGE_ppp-mod-pptp=y

# DDNS 扩展脚本
CONFIG_PACKAGE_ddns-scripts_cloudflare.com-v4=y
CONFIG_PACKAGE_ddns-scripts_dnspod=y

CONFIG_PACKAGE_sqm-scripts-extra=y


# 主题
CONFIG_PACKAGE_luci-theme-argon=y
CONFIG_PACKAGE_luci-theme-netgear=y

# 虚机驱动
CONFIG_PACKAGE_qemu-ga=y
CONFIG_PACKAGE_open-vm-tools=y


#################################
# 测试

